@model IEnumerable<ProjetoFinal.Models.Procedimentorealizado>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Procedimento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Colaborador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Localrealizacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Datarealizacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ObservacaoRealizacaoNome)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cliente.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Procedimento.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Colaborador.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Localrealizacao.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Datarealizacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ObservacaoRealizacaoNome)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                <a asp-action="Imprimir" asp-route-id="@item.Id" target="_blank">Imprimir</a>
            </td>
        </tr>
}
    </tbody>
</table>
